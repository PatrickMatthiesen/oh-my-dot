    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Root",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            // "args": ["."]
        },
        {
            "name": "Launch init",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["init", "-u", "https://github.com/PatrickMatthiesen/dotfiles", "-f", ""]
        },
        {
            "name": "Launch remove",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["remove", ".\\.\\readme.md"],
        },
    ]
}